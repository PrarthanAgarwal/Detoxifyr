{
  "manifest_version": 3,
  "key": "",
  "name": "Detoxifyr 3.0",
  "version": "1.0.0",
  "description": "Personalize and detoxify your YouTube feed",
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "identity",
    "identity.email",
    "downloads"
  ],
  "host_permissions": [
    "https://*.youtube.com/*",
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Detoxifyr"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "oauth2": {
    "client_id": "84661743876-qu25ljp5l5iv9n42g49evgi42mehc7b9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/youtube.readonly",
      "https://www.googleapis.com/auth/youtube.force-ssl"
    ]
  }
}